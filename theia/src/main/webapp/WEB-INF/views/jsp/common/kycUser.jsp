<%@ page trimDirectiveWhitespaces="true" %>
<%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c" %>
<%@ taglib uri="http://java.sun.com/jsp/jstl/functions" prefix="fn" %>
<%@ taglib prefix="fmt" uri="http://java.sun.com/jsp/jstl/fmt" %>
<%@ taglib prefix="ptm" uri="PaytmCustomTags" %>
<%@ include file="config.jsp" %>


<html>
<head>

    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0, user-scalable=no">

    <style type="text/css">
        /* Eric Meyer's Reset CSS v2.0 - http://cssreset.com */
        html,body,div,span,applet,object,iframe,h1,h2,h3,h4,h5,h6,p,blockquote,pre,a,abbr,acronym,address,big,cite,code,del,dfn,em,img,ins,kbd,q,s,samp,small,strike,strong,sub,sup,tt,var,b,u,i,center,dl,dt,dd,ol,ul,li,fieldset,form,label,legend,table,caption,tbody,tfoot,thead,tr,th,td,article,aside,canvas,details,embed,figure,figcaption,footer,header,hgroup,menu,nav,output,ruby,section,summary,time,mark,audio,video{border:0;font:inherit;font-size:14px;color: #4a4a4a;vertical-align:baseline;margin:0;padding:0}article,aside,details,figcaption,figure,footer,header,hgroup,menu,nav,section{display:block}body{line-height:1}ol,ul{list-style:none}blockquote,q{quotes:none}blockquote:before,blockquote:after,q:before,q:after{content:none}table{border-collapse:collapse;border-spacing:0}

        @font-face {
            font-family: 'Open Sans';
            font-style: normal;
            font-weight: 400;
            src: local('Open Sans'), local('OpenSans'), url(https://themes.googleusercontent.com/static/fonts/opensans/v6/cJZKeOuBrn4kERxqtaUH3T8E0i7KZn-EPnyo3HZu7kw.woff) format('woff');
        }
        body{font-family:'Open Sans', Arial, sans-serif; color: #000;}

        /* prevent safari */
        input[type=button], input[type=submit], input[type=text] {
            -webkit-appearance: none;
            -webkit-border-radius: 0;
            border: none; outline: none;
        }

        .bgstript{background: #edf2f7; height: 100px; position: absolute; width: 100%; top:0; left:0;}

        .main{
            width: 800px;
            margin: 0 auto;
            margin-top: 20px;
            background: #fff;
            box-shadow: #a0a0a0 1px 0px 2px 1px;
            min-height: 400px;
            border-radius: 4px;
            z-index: 9;
            position: relative;
        }
        .header{width: 100%;
            padding: 10px;
            clear: both;
            border-bottom: 1px solid #ccc;
            box-sizing: border-box;
            -moz-box-sizing: border-box;
            -webkit-box-sizing: border-box;
        }
        .clear{clear: both;}
        .paytmLogo{
            background: url('data:image/png;base64,') ;
            width: 100px; height: 32px;

            background-repeat: no-repeat;
            padding-top: 10px;
            margin-top: 10px;

        }
        .kycBox{
            padding: 20px 30px;
        }
        .kycBox ul{display: block; clear: both; }
        .kycBox ul li{     width: 48%;
            box-sizing: border-box;
            float: left;
            margin-right: 5px; }
        .kycHeading{
            font-size: 25px; font-weight: bold; margin-bottom: 20px;
        }
        .kycNote{
            line-height: 24px;
            font-size: 16px;
            margin-bottom: 20px;
        }
        .confirmBox{margin-top: 30px; width: 50%; position: relative;}

        .kycBox input[type="radio"]{
            display:none;
        }

        .kycBox input[type="radio"] + label:before {
            content:url('data:image/png;base64,');
            background-repeat: no-repeat;
            height: 22px;
            width: 20px;
            display:inline-block;
            padding: 0 0 0 0px;
            cursor:pointer;
            margin-right: 10px;
            margin-top: 6px;
            position: relative;
            top:4px;
        }
        .kycBox label{width: 100%; display: block;}

        .kycBox input[type="radio"]:checked + label:before {
            content:url('data:image/png;base64,');
            background-repeat: no-repeat;
        }
        .confirmBox input[name="confirm"]{display: none;}

        .confirmBox input[name="confirm"] + label:before {
            content:url('data:image/png;base64,');
            background-repeat: no-repeat;
            height: 19px;
            width: 20px;
            display:inline-block;
            padding: 0 0 0 0px;
            cursor:pointer;
            margin-right: 10px;
            margin-top: 6px;
            position: absolute;
            top: -7px;
            left: 0;
        }
        .confirmBox label{
            padding-left: 27px;
            line-height: 18px;
        }

        .kycData{margin: 25px 10px;}
        .kycField{width: 40%;margin-top: 10px;}
        .kycField input[type='text']{width: 100%; padding:8px 8px 8px 0px; font-size: 16px; border-bottom: 1px solid #ccc;}

        .kycField input[type='text']:focus{ border-bottom: 2px solid #4cbaf6;}
        .submitBtn{
            background: #4cbaf6;
            width: 308px;
            padding: 14px;
            color: #fff;
            text-align: center;
            margin-top: 20px;
            font-size: 17px;
            border-radius: 4px !important;
        }

        .hide {
            display: none;
        }

        .show{
            display: block;
        }

        .error {
            color: red;
        }
        .notification {
            padding: 10px;
            line-height : 20px;
            background-color: #f2dede;
            border:1px solid #ebccd1 ;
            color: #a94442;
            border-radius: 4px;
            margin-bottom: 15px;
            font-size : 16px;
            font-weight : bold;
        }

        @media (max-width: 767px) {
            .main{box-shadow: none; width: 100%; margin: 0;}
            .kycBox ul li{     width: 100%;
                box-sizing: border-box;
                float: left;
                margin-right: 0; }
            .kycField{width: 100%;margin-top: 10px;}
            .confirmBox{width: 98%;}
            .submitBtn{
                width: 100%;
            }
        }

    </style>
</head>
<body>
<div class="bgstript"></div>
<div class="main">
    <div class="header">
        <div class="paytmLogo"></div>
        <div class="clear"></div>
    </div>
    <div class="kycBox">
        <c:if test="${!empty requestScope.KYC_ERROR_MESSAGE}">
            <div class="notification">
                ${requestScope.KYC_ERROR_MESSAGE}
            </div>
        </c:if>
        <div class="kycHeading">Important</div>
        <div class="kycNote">As per RBI guidelines, please provide any of the following document details to continue using Paytm Wallet.</div>
        <div class="kycForm">
            <div class="error hide" id="errorMsg"></div>
            <ul>
                <%--<li>--%>
                    <%--<input type="radio" name="documentType" value="aadhar" id="aadhar"/>--%>
                    <%--<label id="aadhaarDocumentType" onclick="app.onDocumentTypeSelected(this)" for="aadhar">Aadhaar Card</label>--%>
                <%--</li>--%>
                <li>
                    <input type="radio" name="documentType" value="voter" id="voter"/>
                    <label onclick="app.onDocumentTypeSelected(this)" id="voterDocumentType"  for="voter">Voter ID</label>
                </li>
                <li>
                    <input type="radio" name="documentType" value="pan" id="pan"/>
                    <label onclick="app.onDocumentTypeSelected(this)" for="pan">PAN Card</label>
                </li>
                <li>
                    <input type="radio" name="documentType" value="dl" id="dl"/>
                    <label onclick="app.onDocumentTypeSelected(this)" for="dl">Driving License</label>
                </li>
                <li>
                    <input type="radio" name="documentType" value="passport" id="passport"/>
                    <label onclick="app.onDocumentTypeSelected(this)" for="passport">Passport</label>
                </li>
                <li>
                    <input type="radio" name="documentType" value="nrega_job" id="nrega_job"/>
                    <label onclick="app.onDocumentTypeSelected(this)" for="nrega_job">NREGA Job Card</label>
                </li>
            </ul>
            <div class="clear"></div>
            <div id="submitData" class="hide">
                <form action="${pageContext.request.contextPath}/payment/request/submit?${queryStringForSession}" method="post" onsubmit="return app.validateKYCForm();">
                    <div class="kycData">
                        <div class="kycField">
                            <input type="text" id="documentValue" name="KYC_DOC_VALUE" value="" placeholder="" class="box borderEffect" />
                        </div>
                        <div class="kycField">
                            <input type="text" id="kycUserName" name="KYC_NAME_ON_DOC" value="" placeholder="" class="box borderEffect" />
                        </div>
                        <div class="clear"></div>
                    </div>
                    <div class="confirmBox">
                        <input type="checkbox" name="confirm" value="Y" checked id="confirmTnc" disabled />
                        <label for="confirmTnc">
                            I confirm that the above details are correct and can be used for completing my minimum KYC.
                        </label>
                    </div>
                    <input type="hidden" id="KYC_MID" name="KYC_MID" value="${requestScope.KYC_MID}"/>
                    <input type="hidden" id="KYC_ORDER_ID" name="KYC_ORDER_ID" value="${requestScope.KYC_ORDER_ID}"/>
                    <input type="hidden" id="KYC_FLOW" name="KYC_FLOW" value="yes"/>
                    <input type="hidden" id="KYC_TXN_ID" name="KYC_TXN_ID" value="${requestScope.KYC_TXN_ID}"/>
                    <input type="hidden" id="KYC_USER_ID" name="KYC_USER_ID" value="${requestScope.KYC_USER_ID}"/>
                    <input type="hidden" id="documentCode" name="KYC_DOC_CODE" value=""/>

                    <c:if test="${csrfToken.token != null}">
                        <c:set var="varCSRF" value="${csrfToken.token}" scope="session"></c:set>
                    </c:if>

                    <input type="hidden" id="csrfToken" name="CSRF_PARAM" value="${varCSRF}"/>

                    <div>
                        <input type="submit" value="Submit" class="submitBtn"/>
                    </div>
                </form>
            </div>
            <div class="clear"></div>
        </div>
    </div>
</div>

<script>

    var app = {};
    app.constant = {
        "AADHAR":"aadhar",
        "PAN_CARD":"pan",
        "PASSPORT":"passport",
        "VOTER_ID":"voter",
        "DRIVING_LICENSE":"dl",
        "NREGA":"nrega_job"
    };

    app.regex = {
        "aadhar":/^\d{12}$/,
        "panCard":/[a-zA-Z]{3}[Pp]{1}[a-zA-Z]{1}\d{4}[a-zA-Z]{1}/,
        "passport":/[a-zA-Z]{1}\d{7}$/,
        "voterId": /^[a-zA-Z]{3}[0-9]{7}$/,
        "drivingLicense":/[a-zA-Z0-9 \\-\\\\(\\)\\.]+/,
        "nregaJob":/[a-zA-Z0-9 \\-\\\\(\\)\\.]+/
    };

    app.errorMsg = {
        "AADHAR":"Aadhaar number not valid",
        "PAN_CARD":"PAN Card number not valid",
        "PASSPORT":"Passport number not valid",
        "VOTER_ID":"Voter ID number not valid",
        "DRIVING_LICENSE":"Driving License number not valid",
        "NREGA":"NREGA Job Card number not valid"
    };

    app.placeholderMsg = {
        "AADHAR":{"DocumentValue":"Enter Aadhaar Card Number", "KycUser":"Enter Name as per Aadhaar"},
        "PAN_CARD":{"DocumentValue":"Enter PAN Card Number", "KycUser":"Enter Name as per PAN Card"},
        "PASSPORT":{"DocumentValue":"Enter Passport Number", "KycUser":"Enter Name as per Passport"},
        "VOTER_ID":{"DocumentValue":"Enter Voter ID Number", "KycUser":"Enter Name as per Voter ID"},
        "DRIVING_LICENSE":{"DocumentValue":"Enter Driving License Number", "KycUser":"Enter Name as per Driving License"},
        "NREGA":{"DocumentValue":"Enter NREGA Job Card Number", "KycUser":"Enter Name as per NREGA Job Card"}
    };

    app.isWAP = (function() {
        var check = false;
        (function(a){if(/(android|bb\d+|meego).+mobile|avantgo|bada\/|blackberry|blazer|compal|elaine|fennec|hiptop|iemobile|ip(hone|od)|iris|kindle|lge |maemo|midp|mmp|mobile.+firefox|netfront|opera m(ob|in)i|palm( os)?|phone|p(ixi|re)\/|plucker|pocket|psp|series(4|6)0|symbian|treo|up\.(browser|link)|vodafone|wap|windows ce|xda|xiino/i.test(a)||/1207|6310|6590|3gso|4thp|50[1-6]i|770s|802s|a wa|abac|ac(er|oo|s\-)|ai(ko|rn)|al(av|ca|co)|amoi|an(ex|ny|yw)|aptu|ar(ch|go)|as(te|us)|attw|au(di|\-m|r |s )|avan|be(ck|ll|nq)|bi(lb|rd)|bl(ac|az)|br(e|v)w|bumb|bw\-(n|u)|c55\/|capi|ccwa|cdm\-|cell|chtm|cldc|cmd\-|co(mp|nd)|craw|da(it|ll|ng)|dbte|dc\-s|devi|dica|dmob|do(c|p)o|ds(12|\-d)|el(49|ai)|em(l2|ul)|er(ic|k0)|esl8|ez([4-7]0|os|wa|ze)|fetc|fly(\-|_)|g1 u|g560|gene|gf\-5|g\-mo|go(\.w|od)|gr(ad|un)|haie|hcit|hd\-(m|p|t)|hei\-|hi(pt|ta)|hp( i|ip)|hs\-c|ht(c(\-| |_|a|g|p|s|t)|tp)|hu(aw|tc)|i\-(20|go|ma)|i230|iac( |\-|\/)|ibro|idea|ig01|ikom|im1k|inno|ipaq|iris|ja(t|v)a|jbro|jemu|jigs|kddi|keji|kgt( |\/)|klon|kpt |kwc\-|kyo(c|k)|le(no|xi)|lg( g|\/(k|l|u)|50|54|\-[a-w])|libw|lynx|m1\-w|m3ga|m50\/|ma(te|ui|xo)|mc(01|21|ca)|m\-cr|me(rc|ri)|mi(o8|oa|ts)|mmef|mo(01|02|bi|de|do|t(\-| |o|v)|zz)|mt(50|p1|v )|mwbp|mywa|n10[0-2]|n20[2-3]|n30(0|2)|n50(0|2|5)|n7(0(0|1)|10)|ne((c|m)\-|on|tf|wf|wg|wt)|nok(6|i)|nzph|o2im|op(ti|wv)|oran|owg1|p800|pan(a|d|t)|pdxg|pg(13|\-([1-8]|c))|phil|pire|pl(ay|uc)|pn\-2|po(ck|rt|se)|prox|psio|pt\-g|qa\-a|qc(07|12|21|32|60|\-[2-7]|i\-)|qtek|r380|r600|raks|rim9|ro(ve|zo)|s55\/|sa(ge|ma|mm|ms|ny|va)|sc(01|h\-|oo|p\-)|sdk\/|se(c(\-|0|1)|47|mc|nd|ri)|sgh\-|shar|sie(\-|m)|sk\-0|sl(45|id)|sm(al|ar|b3|it|t5)|so(ft|ny)|sp(01|h\-|v\-|v )|sy(01|mb)|t2(18|50)|t6(00|10|18)|ta(gt|lk)|tcl\-|tdg\-|tel(i|m)|tim\-|t\-mo|to(pl|sh)|ts(70|m\-|m3|m5)|tx\-9|up(\.b|g1|si)|utst|v400|v750|veri|vi(rg|te)|vk(40|5[0-3]|\-v)|vm40|voda|vulc|vx(52|53|60|61|70|80|81|83|85|98)|w3c(\-| )|webc|whit|wi(g |nc|nw)|wmlb|wonu|x700|yas\-|your|zeto|zte\-/i.test(a.substr(0,4))) check = true;})(navigator.userAgent||navigator.vendor||window.opera);
        return check;
    })();

    app.getDocumentType = function () {
        var ele = document.querySelector('input[name="documentType"]:checked');
        return ele ? ele.value : null;
    };

    app.showErrorMsg = function (msg) {
        var errorMsgEle = document.getElementById("errorMsg");
        if(errorMsgEle){
            errorMsgEle.innerText = msg;
            errorMsgEle.className = errorMsgEle.className.replace(/hide/g,"");
        }
    };

    app.hideErrorMsg = function () {
        var errorMsgEle = document.getElementById("errorMsg");
        if(errorMsgEle){
            errorMsgEle.innerText = "";
            errorMsgEle.classList.add("hide");
        }
    };

    app.removeSpace = function (value) {
        if(value){
            return value.replace(/\s/g,"");
        }
        return "";
    };

    app.validateKYCForm = function () {
        var documentType = app.getDocumentType(),
            isKYCFormValid = false,documentValue = "",
            documentValueEle = document.getElementById("documentValue"),
            kycUserNameEle = document.getElementById("kycUserName");

        if(documentValueEle){
            documentValue = app.removeSpace(documentValueEle.value);
        }

        if(!documentType){
            // Document type not selected
            app.showErrorMsg("Please select any document");
            return isKYCFormValid;
        }

        switch (documentType){
            case app.constant.AADHAR:
                isKYCFormValid = app.regex.aadhar.test(documentValue);
                if(!isKYCFormValid){
                    app.showErrorMsg(app.errorMsg.AADHAR);
                }
                break;

            case app.constant.PAN_CARD:
                isKYCFormValid = app.regex.panCard.test(documentValue);
                if(!isKYCFormValid){
                    app.showErrorMsg(app.errorMsg.PAN_CARD);
                }
                break;

            case app.constant.PASSPORT:
                isKYCFormValid = app.regex.passport.test(documentValue);
                if(!isKYCFormValid){
                    app.showErrorMsg(app.errorMsg.PASSPORT);
                }
                break;

            case app.constant.VOTER_ID:
                isKYCFormValid = app.regex.voterId.test(documentValue);
                if(!isKYCFormValid){
                    app.showErrorMsg(app.errorMsg.VOTER_ID);
                }
                break;

            case app.constant.DRIVING_LICENSE:
                isKYCFormValid = app.regex.drivingLicense.test(documentValue);
                if(!isKYCFormValid){
                    app.showErrorMsg(app.errorMsg.DRIVING_LICENSE);
                }
                break;

            case app.constant.NREGA:
                isKYCFormValid =  app.regex.nregaJob.test(documentValue);
                if(!isKYCFormValid){
                    app.showErrorMsg(app.errorMsg.NREGA);
                }
                break;

            default:
                isKYCFormValid = false;
        }

        if(isKYCFormValid){
            // Check name field
            if(kycUserNameEle){
                isKYCFormValid = kycUserNameEle.value.trim().length > 0;
                if(!isKYCFormValid){
                    // show error for mandatory name field
                    app.showErrorMsg("Name field cannot be blank");
                }
            }
        }

        if(isKYCFormValid){
            // Remove space from field
            if(documentValueEle){
                documentValueEle.value = app.removeSpace(documentValueEle.value);
            }
        }
        return isKYCFormValid;
    };

    app.getPlaceHolderObj = function (type) {
        var output = {
            "documentValue":"",
            "kycUser":""
        };

        if(type){
            switch (type){
                case app.constant.AADHAR:
                    output.documentValue = app.placeholderMsg.AADHAR.DocumentValue;
                    output.kycUser = app.placeholderMsg.AADHAR.KycUser;
                    break;

                case app.constant.PAN_CARD:
                    output.documentValue = app.placeholderMsg.PAN_CARD.DocumentValue;
                    output.kycUser = app.placeholderMsg.PAN_CARD.KycUser;
                    break;

                case app.constant.PASSPORT:
                    output.documentValue = app.placeholderMsg.PASSPORT.DocumentValue;
                    output.kycUser = app.placeholderMsg.PASSPORT.KycUser;
                    break;

                case app.constant.VOTER_ID:
                    output.documentValue = app.placeholderMsg.VOTER_ID.DocumentValue;
                    output.kycUser = app.placeholderMsg.VOTER_ID.KycUser;
                    break;

                case app.constant.DRIVING_LICENSE:
                    output.documentValue = app.placeholderMsg.DRIVING_LICENSE.DocumentValue;
                    output.kycUser = app.placeholderMsg.DRIVING_LICENSE.KycUser;
                    break;

                case app.constant.NREGA:
                    output.documentValue = app.placeholderMsg.NREGA.DocumentValue;
                    output.kycUser = app.placeholderMsg.NREGA.KycUser;
                    break;

                default:
                    output.documentValue = "";
                    output.kycUser = "";
                    break;
            }
        }
        return output;
    };

    app.onDocumentTypeSelected = function(ele) {
        var val,documentValueEle,kycUserNameEle,placeHolderObj = {},submitBlockStr = "",
            submitBlock = document.getElementById("submitData"),documentCodeEle;

        if(app.isWAP){
            // Mobile
            if(submitBlock){
                submitBlockStr = submitBlock.outerHTML;
                if(submitBlock.parentNode && submitBlock.parentNode.removeChild){
                    submitBlock.parentNode.removeChild(submitBlock);
                }

                if(ele){
                    var ht = document.createElement("div");
                    ht.innerHTML = submitBlockStr;
                    ele.parentElement.appendChild(ht);
                }
                submitBlock = document.getElementById("submitData");
            }
        }

        if(ele && ele.getAttribute){
            val = ele.getAttribute("for");
            if(val){
                placeHolderObj = app.getPlaceHolderObj(val);
                documentCodeEle = document.getElementById("documentCode");

                if(documentCodeEle){
                    documentCodeEle.value = val;
                }

                if(submitBlock){
                    // show details form
                    submitBlock.className = submitBlock.className.replace(/hide/g,"");
                    documentValueEle = submitBlock.querySelector("#documentValue");
                    kycUserNameEle = submitBlock.querySelector("#kycUserName");

                    if(documentValueEle){
                        documentValueEle.placeholder = placeHolderObj.documentValue;
                        if(!app.isWAP){
                            // reset to blank if web theme
                            documentValueEle.value = "";
                        }
                    }

                    if(kycUserNameEle){
                        kycUserNameEle.placeholder = placeHolderObj.kycUser;
                        if(!app.isWAP){
                            // reset to blank if web theme
                            kycUserNameEle.value = "";
                        }
                    }
                }
            }
        }

        // Hide error msg
        app.hideErrorMsg();
    };

    (function () {
        // Set default to aadhaar
        var ele = document.getElementById("voterDocumentType");
        if(ele){
            ele.click();
        }
    })();

</script>
</body>
</html>
